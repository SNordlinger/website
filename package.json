  "license": "MIT",
  "dependencies": {
    "prismjs": "^1.17.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^6.26.3",
    "babel-plugin-prismjs": "^1.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.12.3"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      [
        "prismjs",
